#-------------------------------------------------------------------------
units(length = "cm", time = "s", quantity = "mol", act_energy = "J/mol")
#-------------------------------------------------------------------------
# define an artificial element with the molar mass of species A and B (or C)
#-------------------------------------------------------------------------
element(symbol="a", atomic_mass=31)
#-------------------------------------------------------------------------
# define models
#-------------------------------------------------------------------------
ideal_gas(name = "default",
    elements = " a ",
    species = """ R  P""",
    reactions = "all",
    initial_state = state(temperature = 1000.0,
    pressure = OneAtm) )
#-------------------------------------------------------------------------
# Species thermodynamic data
#--------------------------------------------------------------------------

# thermo        = const_cp(h0 =(0.0, 'J/mol'), s0 = (0.0, 'J/mol/K')),

species( name = "R",
    atoms = "a:1.0",
    thermo = const_cp(h0 =(147.86999999999998e3, 'J/mol'), t0 = (300.0, 'K'), cp0 = (46.19356050546696, 'J/mol/K')),
    note = "constant cp with gamma = 1.2195" )

species( name = "P",
    atoms = " a:1.0 ",
    thermo = const_cp(h0 =(0, 'J/mol'), t0 = (300.0, 'K'),  cp0 = (46.19356050546696, 'J/mol/K')),
    note = "constant cp with gamma = 1.2195, q = 4.77 MJ/kg" )
